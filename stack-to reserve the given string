#include<iostream>
#include<string>
using namespace std;
class stacks{
    public:
    string data;
    stacks*next;
    stacks*top=NULL;


void push(char data)
{
    stacks*temp=new stacks();
    temp->data=data;
    temp->next=top;
    top=temp;
}

void display()
{
    stacks*temp=top;
    while(temp!=NULL)
    {
        cout<<temp->data;
        temp=temp->next;
    }
}

    };
int main()
{
    stacks st;
    string a="geeksquiz";
    for(int i=0;i<a.length(),i++)
    {
    st.push(a[i]);
    }
    cout<<"output"<<endl;
    st.display();
    return 0;
}
